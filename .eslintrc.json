{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "eslint-plugin-html"
    ],
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent":"error",
        "no-unused-vars": 1,
        "no-use-before-define": 1,
        "no-redeclare": 1,
        "no-console":0
    }
}
